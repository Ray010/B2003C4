@switch (CurrentPage.PhaseNo)
{
    case 0:
        goto default;

    case 1:

        <KakoFragment @bind-Phase1Data="CurrentPage" @bind-DBSourceData="DBData" />
        goto End;

    case 11:
        <DokusyaActivity @bind-CommonPhase1="CurrentPage" @bind-DBSourceData="DBData"  />
        goto End;

    case 12:
        <MeihanActivity @bind-CommonPhase2="CurrentPage" />@* @bind-DBSourceData="DBData" @bind-KoudokuList="KoudokuDummyData" />*@

        End:
            break;

        default:
            <div>ハンドリングエラー:存在しないページ（PhaseNo:@CurrentPage.PhaseNo )</div>
            break;
    }

