<h3>KakoParent</h3>

@inject IJSRuntime jsRuntime
@using B2003C4.Data;
@using B2003C4.Pages.Common;


@*https://chrissainty.com/using-javascript-interop-in-razor-components-and-blazor/*@
@if (CurrentPage.State)
{
    Rewrite();
}



@switch (CurrentPage.PhaseNo)
{


    case 0:
        goto default;

    case 1:

        <KakoFragment @bind-Phase1Data="CurrentPage" @bind-DBSourceData="DBData" @bind-SearchResultData="ResultData" />
        goto End;

    case 2:
        <DokusyaActivity @bind-CommonPhase1="CurrentPage" @bind-DBSourceData="DBData" @bind-KoudokuList="KoudokuDummyData" />
        goto End;


    End:
        write();
        break;


    default:

        <div>ハンドリングエラー:存在しないページ（PhaseNo:@formDataModel.S_PhoneNo )</div>
        break;

}

@*public EventCallback<string> PageNameChanged { get; set; }で変えた際はyearも変わる*@


@code {
    private FormSearchDataModel formDataModel = new FormSearchDataModel();
    private DummyDataModel DBData = new DummyDataModel();
    private KoudokuDummyData KoudokuDummyData = new KoudokuDummyData();
    private FormSearchDataModel ResultData = new FormSearchDataModel();
}

