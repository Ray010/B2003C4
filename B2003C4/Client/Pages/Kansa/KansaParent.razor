
@page "/Kansa/KansaParent"
@using B2003C4.Client.Pages.Common;

    <h3>KansaParent</h3>

    @switch (formDataModel.PhaseNo)
    {
        case 0:
            goto default;

        case 1:
            <KansaSearchFragment @bind-Phase1Data="formDataModel" @bind-DBSourceData="DBData" />
            goto default;

        case 2:
            <SearchActivity @bind-Phase2Data="formDataModel" @bind-SearchResultData="ResultData" @bind-DBSourceData="DBData" />
            goto default;

        case 3:
            <DokusyaActivity @bind-CommonPhase1="ResultData" @bind-DBSourceData="DBData" />
            goto default;


        default:

            break;

    }


    @formDataModel.ToString()

    @*public EventCallback<string> PageNameChanged { get; set; }で変えた際はyearも変わる*@


    @code {
        private FormSearchDataModel formDataModel = new FormSearchDataModel();
        private DummyDataModel DBData = new DummyDataModel();
        private FormSearchDataModel ResultData = new FormSearchDataModel ();
    }


